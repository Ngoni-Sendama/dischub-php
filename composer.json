{
    "name": "ngoni/dischub-php",
    "description": "A Laravel package for integrating with Dischub API",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ngoni\\DischubPhp\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ngoni\\DischubPhp\\DischubServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
